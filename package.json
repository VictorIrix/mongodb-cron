{
  "name": "mongodb-cron",
  "version": "1.3.0",
  "description": "MongoDB collection as crontab",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "ava": {
    "files": [
      "./dist/src/tests/*.test.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; tsc",
    "example": "npm run build && node dist/example",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublish": "npm run build",
    "test": "npm run build && ava --verbose",
    "speedtest": "babel-node ./speedtest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/mongodb-cron.git"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "database",
    "nosql",
    "cron",
    "schedule",
    "scheduling",
    "queue",
    "job",
    "jobs",
    "collection",
    "collections",
    "capped"
  ],
  "author": "Xpepermint (Kristijan Sedlak)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpepermint/mongodb-cron/issues"
  },
  "homepage": "https://github.com/xpepermint/mongodb-cron#readme",
  "devDependencies": {
    "@types/dot-object": "^1.5.0",
    "@types/later": "^1.2.4",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.10",
    "@types/node": "^10.11.4",
    "ava": "^0.25.0",
    "mongodb": "^3.1.6",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "peerDependencies": {
    "mongodb": "^3.0.0"
  },
  "dependencies": {
    "dot-object": "^1.7.0",
    "es6-sleep": "^2.0.2",
    "later": "^1.2.0",
    "moment": "^2.22.2"
  }
}
